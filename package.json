{
  "name": "ev-eleventy-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "startX": "npx @11ty/eleventy --serve & postcss styles/main.css --o _temp/main.css --watch",
    "css": "postcss styles/main.css --o _temp/main.css --verbose",
    "start": "npx @11ty/eleventy --serve",
    "build": "ELEVENTY_PRODUCTION=true eleventy & NODE_ENV=production postcss styles/main.css --o _site/main.css & uglifyjs-folder scripts -o _site/ -x .js -e",
    "clean": "rm -r _site _temp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esharri2/ev-eleventy-starter.git"
  },
  "keywords": [],
